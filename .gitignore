cat > .gitignore << 'EOF'
# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
yarn.lock

# Binaries and executables
bin/
ffmpeg/
ffmpeg-*/
ffprobe
ffplay
*.exe
*.dll
*.so
*.dylib

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Session and temporary data
session/
sessions/
*.session
*.session.*
temp/
tmp/
baileys_store.json

# Logs
logs/
*.log

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory
coverage/
.nyc_output/

# Build artifacts
dist/
build/
out/
.next/
.nuxt/

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~
*.code-workspace

# Temp files
*.tmp
*.temp

# Large files (GitHub has 100MB limit)
*.zip
*.tar.gz
*.jar
*.iso
*.dmg

# Your specific large files
gh.exe
libvips-42.dll
EOF